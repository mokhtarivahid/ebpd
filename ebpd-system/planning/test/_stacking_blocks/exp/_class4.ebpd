  (:goal (and
                  (top pallet1 p1)
                  (top b10 p2)
                  (on b1 pallet2)
                  (on b2 b1)
                  (on b3 b2)
                  (on b4 b3)
                  (on b5 b4)
                  (on b6 b5)
                  (on b7 b6)
                  (on b8 b7)
                  (on b9 b8)
                  (on b10 b9)))

)